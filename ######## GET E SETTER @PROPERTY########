######## GET E SETTER @PROPERTY########

class Itens:
    def __init__(self):
        self.__id = 0
        self.__nome = ''
        self.__quantidade = 0

# GET and SETTER id
    @property
    def id(self):
        return self.__id

    @id.setter
    def id(self, id):
        self.__id = id

# GET and SETTER nome
    @property
    def nome(self):
        return self.__nome

    @nome.setter
    def nome(self, nome):
        self.__nome = nome

# GET and SETTER quantidade
    @property
    def quantidade(self):
        return self.__quantidade

    @quantidade.setter
    def quantidade(self, quantidade):
        self.__quantidade = quantidade